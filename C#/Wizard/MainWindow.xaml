<Base:BaseWindow  x:Class="Wizard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wizard"
        xmlns:Base="clr-namespace:ThemeManager.Controls;assembly=ThemeManager"
        mc:Ignorable="d"
        Height="500" Background="{DynamicResource WindowBackgroundBrush}" Topmost="True" Closed="BaseWindow_Closed" MinHeight="200" BorderBrush="White" BorderThickness="0.5" Width="700" WindowStartupLocation="CenterScreen" Initialized="BaseWindow_Initialized">
    <Grid>
        <Border Margin="10,10"  CornerRadius="10">
            <Grid Margin="10">
                <TextBlock Text="1.0"  HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
            </Grid>
        </Border>
        <Frame Grid.RowSpan="3" x:Name="frame" NavigationUIVisibility="Hidden"/>
    </Grid>
</Base:BaseWindow>
