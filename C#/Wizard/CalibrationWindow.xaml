<Base:BaseWindow x:Class="ConfigSetting.CalibrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ConfigSetting"
        xmlns:Base="clr-namespace:ThemeManager.Controls;assembly=ThemeManager"   
        mc:Ignorable="d"
        Height="450" Width="800" Background="{DynamicResource WindowBackgroundBrush}"  Topmost="True" WindowStyle="ToolWindow">
    <Grid>
        <StackPanel Width="125">
            <Button Content="相机触发校准"  Click="Button_Click" Tag="0" />
