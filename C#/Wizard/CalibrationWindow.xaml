            <Button Content="放大倍数" Click="Button_Click" Tag="11"/>
            <Button Content="回城校准" Click="Button_Click" Tag="12"/>

            <Button Content="零点校准" Click="Button_Click" Tag="13"/>
            <Button Content="灯光中心点校准" Click="Button_Click" Tag="20"/>
            <Button Content="光源完整性校准" Click="Button_Click" Tag="21"/>
            <Button Content="白平衡校准" Click="Button_Click" Tag="22"/>
            <Button Content="光源明场暗场半径校准" Click="Button_Click" Tag="23"/>
