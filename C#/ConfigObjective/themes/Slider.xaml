    <Style x:Key="LM_Slider" TargetType="Slider" BasedOn="{StaticResource Base_Slider}">
        <Setter Property="Focusable" Value="True"/>
        <Setter Property="SnapsToDevicePixels" Value="true" />
        <Setter Property="OverridesDefaultStyle" Value="true" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="TickFrequency" Value="1"/>

        <Setter Property="IsSnapToTickEnabled" Value="True"/>
        <Setter Property="LargeChange" Value="2"/>
        <Setter Property="SmallChange" Value="1"/>
        <Style.Triggers>
            <Trigger Property="Orientation" Value="Horizontal">
                <Setter Property="MinHeight" Value="21" />
                <Setter Property="MinWidth" Value="70" />
                <Setter Property="Template" Value="{StaticResource LM_SliderControlTemplate}"/>
            </Trigger>
            <Trigger Property="Orientation" Value="Vertical">
                <Setter Property="MinHeight" Value="70" />
                <Setter Property="MinWidth" Value="21" />
                <Setter Property="Template" Value="{StaticResource Slider2}" />
            </Trigger>
        </Style.Triggers>
    </Style>
